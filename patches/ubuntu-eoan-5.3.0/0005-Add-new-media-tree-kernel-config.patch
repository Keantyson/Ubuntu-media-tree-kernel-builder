+# CONFIG_MEDIA_SUPPORT_FILTER is not set
+# CONFIG_VIDEO_HI556 is not set
+# CONFIG_VIDEO_IMX219 is not set
+# CONFIG_VIDEO_IMX290 is not set
+# CONFIG_INTEL_ATOMISP is not set
+CONFIG_IR_TOY=m
+CONFIG_CEC_CH7322=m
+# CONFIG_VIDEO_OV2740 is not set
